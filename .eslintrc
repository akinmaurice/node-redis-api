{
  "extends": "airbnb",
  "env": {
      "browser": true,
      "node": true,
      "jquery": true,
      "es6": true
  },
  "rules": {
      "no-console": 0,
      "import": 0,
      "consistent-return": 0,
      "no-underscore-dangle": 0,
      "func-names": 0,
      "no-plusplus": ["error", {
          "allowForLoopAfterthoughts": true
      }]
  }
}